<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2022-02-04T08:51:30Z</date>
    <groups>
        <group>
            <name>Коммутаторы направления</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Alcatel</template>
            <name>Шаблон Alcatel</name>
            <groups>
                <group>
                    <name>Коммутаторы направления</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Блоки питания</name>
                </application>
                <application>
                    <name>Входящий трафик</name>
                </application>
                <application>
                    <name>Исходящий трафик</name>
                </application>
                <application>
                    <name>Пинг</name>
                </application>
                <application>
                    <name>Статус</name>
                </application>
                <application>
                    <name>Температура</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Температура</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>SNMPv2-SMI::enterprises.6486.800.1.2.1.16.1.1.1.17.0</snmp_oid>
                    <key>alcatel-temperature</key>
                    <delay>3m</delay>
                    <history>7d</history>
                    <trends>7d</trends>
                    <applications>
                        <application>
                            <name>Температура</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                            <params>1800</params>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}&gt;57</expression>
                            <name>Высокая температура</name>
                            <priority>AVERAGE</priority>
                        </trigger>
                        <trigger>
                            <expression>{last()}&gt;70</expression>
                            <name>Очень высокая температура</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Пинг</name>
                    <type>SIMPLE</type>
                    <key>icmpping[{HOST.IP},3,,,]</key>
                    <delay>10s</delay>
                    <history>7d</history>
                    <trends>7d</trends>
                    <applications>
                        <application>
                            <name>Пинг</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED</type>
                            <params/>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}=0</expression>
                            <name>Нет пинга</name>
                            <priority>DISASTER</priority>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Блок питания</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#IFSTATUS},SNMPv2-SMI::enterprises.6486.800.1.1.1.1.1.1.1.2,{#IFNAME},SNMPv2-SMI::mib-2.47.1.1.1.1.7]</snmp_oid>
                    <key>bp-status</key>
                    <delay>3m</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#IFNAME}</macro>
                                <value>^((?!PS).)*$</value>
                                <operator>NOT_MATCHES_REGEX</operator>
                                <formulaid>A</formulaid>
                            </condition>
                            <condition>
                                <macro>{#IFSTATUS}</macro>
                                <value>1</value>
                                <formulaid>B</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Состояние блока питания-{#IFNAME}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>SNMPv2-SMI::enterprises.6486.800.1.1.1.1.1.1.1.2.{#SNMPINDEX}</snmp_oid>
                            <key>bpstate-[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <history>7d</history>
                            <trends>7d</trends>
                            <applications>
                                <application>
                                    <name>Блоки питания</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>DISCARD_UNCHANGED</type>
                                    <params/>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;1</expression>
                                    <name>Проблема с блоком питания - {#IFNAME}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>auto-discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#IFSTATUS},IF-MIB::ifOperStatus,{#IFNAME},IF-MIB::ifName,{#IFALIAS},IF-MIB:ifAlias]</snmp_oid>
                    <key>keyDiscoveryAlcatel</key>
                    <delay>3m</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#IFNAME}</macro>
                                <value>[0-9]*/[0-9]*</value>
                                <formulaid>A</formulaid>
                            </condition>
                            <condition>
                                <macro>{#IFSTATUS}</macro>
                                <value>1</value>
                                <formulaid>B</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Порт-{#IFNAME}({#IFALIAS})-входящие-ошибки</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>ifInErrors.{#SNMPINDEX}</snmp_oid>
                            <key>PortInErrors-[{#IFNAME}]</key>
                            <delay>10s</delay>
                            <history>7d</history>
                            <trends>7d</trends>
                            <applications>
                                <application>
                                    <name>Входящий трафик</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                                    <params>60s</params>
                                </step>
                                <step>
                                    <type>SIMPLE_CHANGE</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Порт-{#IFNAME}({#IFALIAS})-входящие-потери</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>ifInDiscards.{#SNMPINDEX}</snmp_oid>
                            <key>PortInLost-[{#IFNAME}]</key>
                            <history>7d</history>
                            <trends>7d</trends>
                            <applications>
                                <application>
                                    <name>Входящий трафик</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                                    <params>180s</params>
                                </step>
                                <step>
                                    <type>SIMPLE_CHANGE</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Порт-{#IFNAME}({#IFALIAS})-входящий-трафик</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>ifInOctets.{#SNMPINDEX}</snmp_oid>
                            <key>PortInOctets-[{#IFNAME}]</key>
                            <delay>10s</delay>
                            <history>7d</history>
                            <trends>7d</trends>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Входящий трафик</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Порт-{#IFNAME}({#IFALIAS})-исходящие-ошибки</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>ifOutErrors.{#SNMPINDEX}</snmp_oid>
                            <key>PortOutErrors-[{#IFNAME}]</key>
                            <delay>10s</delay>
                            <history>7d</history>
                            <trends>7d</trends>
                            <applications>
                                <application>
                                    <name>Исходящий трафик</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                                    <params>60s</params>
                                </step>
                                <step>
                                    <type>SIMPLE_CHANGE</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Порт-{#IFNAME}({#IFALIAS})-исходящие-потери</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>ifOutDiscards.{#SNMPINDEX}</snmp_oid>
                            <key>PortOutLost-[{#IFNAME}]</key>
                            <history>7d</history>
                            <trends>7d</trends>
                            <applications>
                                <application>
                                    <name>Исходящий трафик</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                                    <params>180s</params>
                                </step>
                                <step>
                                    <type>SIMPLE_CHANGE</type>
                                    <params/>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Порт-{#IFNAME}({#IFALIAS})-исходящий-трафик</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>ifOutOctets.{#SNMPINDEX}</snmp_oid>
                            <key>PortOutOctets-[{#IFNAME}]</key>
                            <delay>10s</delay>
                            <history>7d</history>
                            <trends>7d</trends>
                            <units>bps</units>
                            <applications>
                                <application>
                                    <name>Исходящий трафик</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>CHANGE_PER_SECOND</type>
                                    <params/>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>8</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                        <item_prototype>
                            <name>Порт-{#IFNAME}({#IFALIAS})-Статус</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>ifOperStatus.{#SNMPINDEX}</snmp_oid>
                            <key>PortStatus-[{#IFNAME}]</key>
                            <delay>10s</delay>
                            <history>7d</history>
                            <trends>7d</trends>
                            <applications>
                                <application>
                                    <name>Статус</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>DISCARD_UNCHANGED_HEARTBEAT</type>
                                    <params>300</params>
                                </step>
                            </preprocessing>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}=2</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{last()}=1</recovery_expression>
                                    <name>Порт-{#IFNAME}({#IFALIAS})-неактивен</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{Template Alcatel:PortInErrors-[{#IFNAME}].sum(#3)}+{Template Alcatel:PortOutErrors-[{#IFNAME}].sum(#3)}&gt;300</expression>
                            <name>Порт-{#IFNAME}({#IFALIAS})-ошибки</name>
                            <priority>HIGH</priority>
                            <manual_close>YES</manual_close>
                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>{Template Alcatel:PortOutLost-[{#IFNAME}].sum(#5)}+{Template Alcatel:PortInLost-[{#IFNAME}].sum(#5)}&gt;5000</expression>
                            <name>Порт-{#IFNAME}({#IFALIAS})-потери</name>
                            <priority>WARNING</priority>
                            <manual_close>YES</manual_close>
                        </trigger_prototype>
                    </trigger_prototypes>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$SNMP_COMMUNITY}</macro>
                    <value>3napr</value>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
